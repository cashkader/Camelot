import camelot, pandas as pd

tables = camelot.read_pdf('Alomit_Chart.pdf', pages='1-end',flavor='stream',table_areas=['0,800,800,0'])

df_list = []

for table_num in range (24):
    df_list.append(tables[table_num].df)

df = pd.concat(df_list)

df.to_excel('df.xlsx', encoding='utf-8', index=False)



